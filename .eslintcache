';\n\n            auth.createUserWithEmailAndPassword(email, password)\n                .then(auth=>{\n                    auth.user.updateProfile(credentials)\n                    .then(()=>{\n                    dispatch(login({\n                        email: auth.user.email,\n                        uid: auth.user.uid,\n                        displayName: auth.user.email.split('@')[0],\n                     }))\n                    })\n                })\n\n        } else {\n            alert('Email or Password did not match \\n Check fields and try again.');\n        }\n    }\n\n    return (\n        <div className='register' >\n            <div className='landing__accountSecion' style={{minHeight: 500}}>\n                {!user? (\n                <form>\n                    <h3>Create an Account:</h3>\n                    <div className='landing__formSubsection'>\n                    <p>Email:</p>\n                    <input \n                        className=\"userEmail\"\n                        placeholder=\"Email\"\n                        value={email}\n                        onChange={e=>setEmail(e.target.value)}\n                    />\n\n                <div className='landing__formSubsection'>\n                    <p>Password:</p>\n                    <input \n                        className='userPassword' \n                        placeholder=\"password\" \n                        type=\"password\" \n                        value={password}\n                        onChange={e=>setPassword(e.target.value)}\n                    />\n                </div>\n\n                <div className='landing__formSubsection'>\n                    <p>Verify Password:</p>\n                    <input \n                    className='verifyPassword' \n                    placeholder=\"Verify password\" \n                    type=\"password\" \n                    value={password2}\n                    onChange={e=>setPassword2(e.target.value)}\n                