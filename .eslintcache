n\nfunction App() {\n\n  const user = useSelector(selectDefault);\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  useEffect(()=>{\n    //log in user if authstate changes\n    auth.onAuthStateChanged(authUser=>{\n      if(authUser){\n        dispatch(login({\n          email: authUser.email,\n          uid: authUser.uid,\n          displayName: authUser.email.split('@')[0],\n        }))\n        console.log('dispatched');\n        history.push('/view')\n      } else {\n        dispatch(logout())\n        console.log('logged out');\n      }\n    })\n  }, [])\n\n  return (\n    <Router>\n      <div className=\"App\">\n        <Switch>\n\n          <Route path='/Post/:postId'>\n            <Header />\n            <Meal />\n          </Route>\n\n          <Route path='/update/:postId'>\n            <Header />\n            <Meal update={true}/>\n          </Route>\n\n          <Route path='/Create'>\n            <Header />\n            <Create />\n          </Route>\n\n          <Route path='/View'>\n            <Header />\n            <View />\n          </Route>\n\n          <Route path='/Main'>\n            <Header />\n            <Main />\n          </Route>\n\n          <Route path='/Basket'>\n            <Header />\n            <Basket />\n          </Route>\n\n          <Route path='/login'>\n            <Header />\n            <Login />\n          </Route>\n\n          <Route path='/user'>\n            <Header />\n            <User />\n          </Route>\n\n          <Route path='/'>\n              <Header />\n              <Landing />\n          </Route>\n\n        </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/home/kam/Documents/projects/groceries/src/Landing.js",["123"],"import React, { useEffect, useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport './Landing.css';\nimport './formStyles.css';\nimport { facts } from './randoFacts';\nimport TopFive from './TopFive';\nimport Register from './Register';\n// import Register from './Register';\nconst API_URL = \"http://localhost:9000\";\n\nconst Landing = () => {\n    const randoIndex = Math.floor((Math.random()* facts.length) +1);\n    const randoQuote = facts[randoIndex];\n\n\n    const [topFive, setTopFive] = useState('');\n\n\n    useEffect(()=>{\n        fetch(API_URL+\"/view\", {\n            method: 'GET',\n        }).then((response)=>{\n            return response.json();\n        }).then((data)=>{\n            setTopFive(data.slice(0,5));\n        })\n    }, [])\n\n    return (\n\n        <div className=\"landing\">\n            <div className='landing__underlay'>\n                <div className='landing__wrapper'>\n                    <div className='landing__titleWrapper'>\n                        <h1>LETS MAKE A LIST!</h1>\n                        <div className='landing__quote'>{randoQuote? randoQuote : 'Welcome!'}</div>\n                    </div>\n\n                    <Register />\n\n                </div>\n            </div>\n            <div className='landing__subsection'>\n                <div className='landing__leftSpacer'>\n                    ad space\n                </div>\n\n                <div className='landing__topFive'>\n                    <h2>Top Five Right Now!</h2>\n                    {topFive? topFive.map((obj, index)=>(\n                        <TopFive \n                            data={obj}\n                            index={index+1}\n                        />\n                    )) : 'top five'}\n                </div>\n\n                <div className='landing__rightSpacer'>\n                    ad space\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Landing\n","/home/kam/Documents/projects/groceries/src/Login.js",[],"/home/kam/Documents/projects/groceries/src/Header.js",["124"],"/home/kam/Documents/projects/groceries/src/stories/Header.stories.js",["125"],"/home/kam/Documents/projects/groceries/src/stories/Header.js",[],["126","127"],"/home/kam/Documents/projects/groceries/src/stories/myHeader.stories.js",["128"],"/home/kam/Documents/projects/groceries/src/randoFacts.js",[],"/home/kam/Documents/projects/groceries/src/Basket.js",["129","130"],"import React from 'react';\nimport \"./Basket.css\";\nimport { useSelector } from 'react-redux';\nimport { selectDefault } from './features/reactSlice';\nimport { selectBasket } from './features/basketSlice';\nimport SmallPost from './SmallPost';\n\nconst Basket = () => {\n\n    const user = useSelector(selectDefault);\n    const basket = useSelector(selectBasket);\n    const reformatedBasket = [];\n\n    if (basket){\n        basket.map(item=>{\n            console.log('item', item);\n            for (let ingrediant of item.ingrediants){\n            reformatedBasket.push([`${item.title}`, ingrediant]);\n            }\n        })\n    }\n\n    console.log(reformatedBasket);\n\n    return (\n        <div className='basket'>\n            <div className='basket__banner'>ad space</div>\n\n            {!user && (<div className='basket__accountReminder'><strong>Notice:</strong>  &nbsp;You are not currently logged in. Log in or create an account so you could save your basket to your account and even send it to mobile!</div>)}\n\n            <div className='basket__controls'>Filters</div>\n\n            <div className='basket__content'>\n\n                <div className='basket__list'>\n                    <h2>List</h2>\n\n                    {reformatedBasket && reformatedBasket.map(meal=>(\n                    <div className='basket__mealWrapper'>\n                        <div className='basket__remove'>\n                            <input type='checkbox' />\n                            <label>Remove</label>\n                        </div>\n                        <div className='basket__meal'>\n                            <div className='basket__ingrediant'>{meal[1].name}</div>\n                            <div className='basket__amount'>{meal[1].amount && meal[1].amount}</div>\n                        </div>\n                        <div className='basket__mealTitle--sm'>{meal[0]}</div>\n                    </div>\n                    ))}\n\n                    {/* {basket && basket.map(mealData=>(\n                        <div className='basket__mealWrapper'>\n                            <div className='basket__remove'>\n                                <input type='checkbox' />\n                                <label>Remove</label>\n                            </div>\n                            {mealData.ingrediants.map(ingrediant=>(\n                                <div className='basket__meal'>\n                                    <div className='basket__ingrediant'>{ingrediant.name}</div>\n                                    <div className='basket__amount'>{ingrediant.amount && ingrediant.amount}</div>\n                                </div>\n                            ))}\n                            <div className='basket__mealTitle--sm'>{mealData.title}</div>\n                        </div>\n                    ))} */}\n\n                </div>\n\n                <div className='basket__meals'>\n                    <h2>Meals Selected</h2>\n                    {basket && basket.map(mealData=>(\n                        <div className='basket__mealTitle'>{mealData.title}</div>\n                    ))}\n                </div>\n            </div>\n\n        </div>\n    )\n}\n\nexport default Basket\n","/home/kam/Documents/projects/groceries/src/Main.js",[],"/home/kam/Documents/projects/groceries/src/Create.js",["131"],"import React, { useState } from 'react';\nimport { Link, useHistory } from 'react-router-dom';\nimport './Create.css';\nimport Ingrediant from './Ingrediant';\nimport AddIcon from '@material-ui/icons/Add';\nimport RemoveIcon from '@material-ui/icons/Remove';\nimport { useSelector } from 'react-redux';\nimport { selectDefault } from './features/reactSlice';\n\nconst API_URL = \"http://localhost:9000\";\n\nconst Create = () => {\n\n    const history = useHistory();\n    const user = useSelector(selectDefault);\n    // where true is privacy for public\n    const [privacySetting, setPrivacySetting] = useState(true);\n    const [imageUrl, setImageUrl] = useState('');\n\n    const submitNewMeal = () =>{\n\n        // grab alert banner\n        let alertBanner = document.getElementsByClassName('alert__banner')[0];\n\n        //mealTitle is already set\n        const ingrediants = [];\n        const sendData = [];\n        // const sendData = new FormData();\n        const ingrediantArray = document.getElementsByClassName('ingrediant__new')\n        for (let elem of ingrediantArray){\n            let amountInput = elem.nextSibling.nextSibling.childNodes[0].value;\n            if (!amountInput){\n                amountInput = 0;\n            }\n            let ingrediantPair = {}\n            ingrediantPair[\"name\"] = elem.value;\n            ingrediantPair[\"amount\"] = amountInput;\n            ingrediants.push(ingrediantPair);\n        }\n\n        // verify fields\n\n        if (!mealTitle){\n            // raise alertbanner, no title given\n            alertBanner.innerText = 'A Title is Required';\n            alertBanner.style.display = 'flex';\n            alertBanner.style.backgroundColor = 'red';\n        }\n\n        if (!ingrediants || ingrediants[0].length===0){\n            // raise alertbanner, no ingredians given\n            alertBanner.innerText = 'You must give at least one ingrediant';\n            alertBanner.style.display = 'flex';\n            alertBanner.style.backgroundColor = 'red';\n        }\n\n        if (mealTitle){\n            if (ingrediants){\n                // all exist\n                alertBanner.innerText = 'Success! Now adding to your creations.';\n                alertBanner.style.display = 'flex';\n                alertBanner.style.backgroundColor = 'green';\n\n                // author, ingrediants, privacy, instructions\n                let sendData = {\n                    \"title\": mealTitle,\n                    \"ingrediants\": ingrediants,\n                    \"instructions\" : directions,\n                    \"privacy\": privacySetting,\n                }\n\n                if (imageUrl){\n                    sendData.image = imageUrl;\n                } else {\n                    sendData.image = '';\n                }\n\n                if (user){\n                    sendData[\"author\"] = user.displayName;\n                } else {\n                    sendData['author'] = \"Anonymous\";\n                }\n                // sendData.append(\"privacy\", true);\n                    // send to server\n                    fetch(API_URL+'/create', {\n                        method:'POST',\n                        body: JSON.stringify(sendData),\n                        headers: {\n                            \"Content-type\": \"application/json; charset=UTF-8\"\n                         },\n                    }).then((response)=>{\n                        return response.json();\n                    }).then((data)=>{\n                        history.push('/view');\n                    })\n            }\n        }\n    }\n\n    let [count, setCount] = useState(1); // index for ing componet\n    let [mealTitle, setTitle] = useState('');\n    let [directions, setDirections] = useState('');\n\n    return (\n        <div className='create'>\n            <Link className=\"create__social\" to='/view'>V</Link>\n\n            <div className='alert__banner'></div>\n\n            <div className='create__wrapper'>\n\n                <div className='create__conversions'>\n                    Conversions chart\n                </div>\n\n                <div className='create__new'>\n                    <form className='create__form'>\n                        <div className='create__titleWrapper'>\n                            <h3>Name your Creation</h3>\n                            <input \n                                className='create__title' \n                                placeholder='Title'\n                                value={mealTitle}\n                                onChange={event=>setTitle(event.target.value)}\n                            />\n                        </div>\n\n                        <div className='create__ingrediants'>\n                            <div className='create__imageWrapper'>\n                                <p>Image Url:</p>\n                                <input className='create__image'\n                                    value={imageUrl}\n                                    onChange={e=>setImageUrl(e.target.value)}\n                                />\n                            </div>\n                            { Array(count).fill().map((num, index)=>(\n                                <Ingrediant ingrediantNumber={index} />\n                                ))\n                            }\n\n                            {count>1 && \n                                <button className='create__button' type=\"button\" onClick={()=>setCount(count-1)}>\n                                    <RemoveIcon /> Remove Last Ingrediant\n                                </button>\n                            }\n                            <button className='create__button' type=\"button\" onClick={()=>setCount(count+1)}><AddIcon /> Add Ingrediant</button>\n                            \n                            <div className='create__instructions'>\n                                <p>How to prepare (this should be a different element)</p>\n                                <input \n                                    placeholder='instructions'\n                                    value={directions}\n                                    onChange={event=>setDirections(event.target.value)}    \n                                />\n                            </div>\n                        </div>\n\n\n                    </form>\n\n                    <div className='create__controls'>\n                        <span>\n                            <input \n                            name='privacy' \n                            type='radio' \n                            id='public'\n                            onClick={()=>setPrivacySetting(true)}\n                            checked />\n                            <label for='public'>Public</label>\n                        </span>\n                        <span>\n                            <input \n                            className='checkbox' \n                            name='privacy' \n                            type='radio' \n                            onClick={()=>setPrivacySetting(false)} \n                            id='private' \n                            />\n                            <label className='checkbox' for='private'>Private</label>\n                        </span>\n                        <button \n                            className='create__buttonSubmit'\n                            onClick={submitNewMeal.bind()}\n                        >\n                                Submit Creation\n                            </button>\n                    </div>\n                    \n                </div>\n\n                <div className='create__view'>\n                    Top 10 last choosen menu items here\n                </div>\n\n            </div>\n        </div>\n    )\n}\n\nexport default Create\n","/home/kam/Documents/projects/groceries/src/Ingrediant.js",[],"/home/kam/Documents/projects/groceries/src/View.js",["132"],"import React, { useState, useEffect } from 'react';\nimport { Link } from 'react-router-dom';\nimport \"./View.css\";\nimport AddIcon from '@material-ui/icons/Add';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { selectDefault } from './features/reactSlice';\nimport { ADD_TO_BASKET, selectBasket } from './features/basketSlice';\n\nconst API_URL = \"http://localhost:9000\";\n\nconst View = () => {\n\n    const user = useSelector(selectDefault);\n    const basket = useSelector(selectBasket);\n    const dispatch = useDispatch();\n    const [posts, setPosts] = useState('');\n    const [forceOnDelete, setForceOnDelete] = useState(0);\n\n    const deletePost = (id)=>{\n        fetch(API_URL+`/postId=${id}`, {\n            method:\"DELETE\",\n        }).then(()=>{\n            setForceOnDelete(forceOnDelete+1);\n        })\n    }\n\n    const addMealToBasket = (singleMealData) =>{\n        // dispatch data to basket\n        dispatch(ADD_TO_BASKET(singleMealData));\n    }\n\n    useEffect(() => {\n        fetch(API_URL+\"/view\", {\n            method: 'GET',\n        }).then((response)=>{\n            return response.json();\n        }).then((data)=>{\n            setPosts(data);\n        })\n    }, [forceOnDelete])\n\n    return (\n        <div className='view'>\n            <Link className='view__create' to='/create'><AddIcon /></Link>\n            {user && \n            <div className='view__user'>\n                <div className='view__userWrapper'>\n                    <img src={user.imageURL} alt='' />\n                    <h4>Welcome back, {user.displayName}</h4>\n                </div>\n            </div>}\n            <div className='view__wrapper'>\n                <h2>View Recent Publishes</h2>\n                {posts? posts.map((obj, index)=>{\n                    if (obj.privacy === true){\n                    return (<Link to={`/post/${obj._id}`}><div className='view__container'>\n                        <div className='view__add' \n                            onClick={()=>addMealToBasket(obj)}\n                        >\n                            +\n                        </div>\n                        {user && user.email.split('@')[0]===obj.author &&\n                            <div className='userControls'>\n                                <div \n                                    className='view__delete view__control'\n                                    onClick={()=> deletePost(obj._id)}\n                                >Delete</div>\n                                <Link to={`/update/${obj._id}`}><div className='view__update view__control'>Update</div></Link>\n                            </div>}\n                        <img \n                            className='view__image'\n                            src={obj.image? obj.image : 'https://images.unsplash.com/photo-1608501857571-31a43311e342?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1950&q=80'}\n                            alt=''\n                        />\n                        <div className='view__content'>\n                        <div className='view__title' >{obj.title}</div>\n                        <div className='view__author' >Published By: {obj.author}</div>\n                        <div className='view__date' >{new Date(parseInt(obj.published_date)).toDateString()}</div>\n                        <div className='view__directions' >\n                            <ul>\n                                {obj.ingrediants? obj.ingrediants.map((data)=>(\n                                    <li>{data.name}{data.amount? \": \"+data.amount:''}</li>\n                                )) : ''}\n                            </ul>\n                            <span>{obj.instructions? obj.instructions : ''}</span>\n                        </div>\n                    </div>\n                    </div></Link>)\n                } else {\n                    return ('')\n                }}) : (\n                <div className='view__container'>\n                    <img className='view__image' \n                        src='https://images.unsplash.com/photo-1608501857571-31a43311e342?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1950&q=80'\n                        alt=''\n                    />\n            