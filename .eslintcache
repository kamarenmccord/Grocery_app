Default);\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  useEffect(()=>{\n    //log in user if authstate changes\n    auth.onAuthStateChanged(authUser=>{\n      if(authUser){\n        dispatch(login({\n          email: authUser.email,\n          uid: authUser.uid,\n          displayName: authUser.email.split('@')[0],\n        }))\n        console.log('dispatched');\n        history.push('/view')\n      } else {\n        dispatch(logout())\n        console.log('logged out');\n      }\n    })\n  }, [])\n\n  return (\n    <Router>\n      <div className=\"App\">\n        <Switch>\n\n          <Route path='/Post/:postId'>\n            <Header />\n            <Meal />\n          </Route>\n\n          <Route path='/update/:postId'>\n            <Header />\n            <Meal update={true}/>\n          </Route>\n\n          <Route path='/Create'>\n            <Header />\n            <Create />\n          </Route>\n\n          <Route path='/View'>\n            <Header />\n            <View />\n          </Route>\n\n          <Route path='/Main'>\n            <Header />\n            <Main />\n          </Route>\n\n          <Route path='/Basket'>\n            <Header />\n            <Basket />\n          </Route>\n\n          <Route path='/login'>\n            <Header />\n            <Login />\n          </Route>\n\n          <Route path='/user'>\n            <Header />\n            <User />\n          </Route>\n\n          <Route path='/'>\n              <Header />\n              <Landing />\n          </Route>\n\n        </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/home/kam/Documents/projects/groceries/src/Landing.js",["121"],"import React, { useEffect, useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport './Landing.css';\nimport './formStyles.css';\nimport { facts } from './randoFacts';\nimport TopFive from './TopFive';\nimport Register from './Register';\n// import Register from './Register';\nconst API_URL = \"http://localhost:9000\";\n\nconst Landing = () => {\n    const randoIndex = Math.floor((Math.random()* facts.length) +1);\n    const randoQuote = facts[randoIndex];\n\n\n    const [topFive, setTopFive] = useState('');\n\n\n    useEffect(()=>{\n        fetch(API_URL+\"/view\", {\n            method: 'GET',\n        }).then((response)=>{\n            return response.json();\n        }).then((data)=>{\n            setTopFive(data.slice(0,5));\n        })\n    }, [])\n\n    return (\n\n        <div className=\"landing\">\n            <div className='landing__underlay'>\n                <div className='landing__wrapper'>\n                    <div className='landing__titleWrapper'>\n                        <h1>LETS MAKE A LIST!</h1>\n                        <div className='landing__quote'>{randoQuote? randoQuote : 'Welcome!'}</div>\n                    </div>\n\n                    <Register />\n\n                </div>\n            </div>\n            <div className='landing__subsection'>\n                <div className='landing__leftSpacer'>\n                    ad space\n                </div>\n\n                <div className='landing__topFive'>\n                    <h2>Top Five Right Now!</h2>\n                    {topFive? topFive.map((obj, index)=>(\n                        <TopFive \n                            data={obj}\n                            index={index+1}\n                        />\n                    )) : 'top five'}\n                </div>\n\n                <div className='landing__rightSpacer'>\n                    ad space\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Landing\n","/home/kam/Documents/projects/groceries/src/Login.js",[],"/home/kam/Documents/projects/groceries/src/Header.js",["122"],"/home/kam/Documents/projects/groceries/src/stories/Header.stories.js",["123"],"/home/kam/Documents/projects/groceries/src/stories/Header.js",[],["124","125"],"/home/kam/Documents/projects/groceries/src/stories/myHeader.stories.js",["126"],"/home/kam/Documents/projects/groceries/src/randoFacts.js",[],"/home/kam/Documents/projects/groceries/src/Basket.js",["127","128"],"import React from 'react';\nimport \"./Basket.css\";\nimport { useSelector } from 'react-redux';\nimport { selectDefault } from './features/reactSlice';\nimport { selectBasket } from './features/basketSlice';\nimport SmallPost from './SmallPost';\n\nconst Basket = () => {\n\n    const user = useSelector(selectDefault);\n    const basket = useSelector(selectBasket);\n    const reformatedBasket = [];\n\n    if (basket){\n        basket.map(item=>{\n            console.log('item', item);\n            for (let ingrediant of item.ingrediants){\n            reformatedBasket.push([`${item.title}`, ingrediant]);\n            }\n        })\n    }\n\n    console.log(reformatedBasket);\n\n    return (\n        <div className='basket'>\n            <div className='basket__banner'>ad space</div>\n\n            {!user && (<div className='basket__accountReminder'><strong>Notice:</strong>  &nbsp;You are not currently logged in. Log in or create an account so you could save your basket to your account and even send it to mobile!</div>)}\n\n            <div className='basket__controls'>Filters</div>\n\n            <div className='basket__content'>\n\n                <div className='basket__list'>\n                    <h2>List</h2>\n\n                    {reformatedBasket && reformatedBasket.map(meal=>(\n                    <div className='basket__mealWrapper'>\n                        <div className='basket__remove'>\n                            <input type='checkbox' />\n                            <label>Remove</label>\n                        </div>\n                        <div className='basket__meal'>\n                            <div className='basket__ingrediant'>{meal[1].name}</div>\n                            <div className='basket__amount'>{meal[1].amount && meal[1].amount}</div>\n                        </div>\n                        <div className='basket__mealTitle--sm'>{meal[0]}</div>\n                    </div>\n                    ))}\n\n                    {/* {basket && basket.map(mealData=>(\n                        <div className='basket__mealWrapper'>\n                            <div className='basket__remove'>\n                                <input type='checkbox' />\n                                <label>Remove</label>\n                            </div>\n                            {mealData.ingrediants.map(ingrediant=>(\n                                <div className='basket__meal'>\n                                    <div className='basket__ingrediant'>{ingrediant.name}</div>\n                                    <div className='basket__amount'>{ingrediant.amount && ingrediant.amount}</div>\n                                </div>\n                            ))}\n                            <div className='basket__mealTitle--sm'>{mealData.title}</div>\n                        </div>\n                    ))} */}\n\n                </div>\n\n                <div className='basket__meals'>\n                    <h2>Meals Selected</h2>\n                    {basket && basket.map(mealData=>(\n                        <div className='basket__mealTitle'>{mealData.title}</div>\n                    ))}\n                </div>\n            </div>\n\n        </div>\n    )\n}\n\nexport default Basket\n","/home/kam/Documents/projects/groceries/src/Main.js",[],"/home/kam/Documents/projects/groceries/src/Create.js",["129"],"/home/kam/Documents/projects/groceries/src/Ingrediant.js",[],"/home/kam/Documents/projects/groceries/src/View.js",["130"],"import React, { useState, useEffect } from 'react';\nimport { Link } from 'react-router-dom';\nimport \"./View.css\";\nimport AddIcon from '@material-ui/icons/Add';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { selectDefault } from './features/reactSlice';\nimport { ADD_TO_BASKET, selectBasket } from './features/basketSlice';\n\nconst API_URL = \"http://localhost:9000\";\n\nconst View = () => {\n\n    const user = useSelector(selectDefault);\n    const basket = useSelector(selectBasket);\n    const dispatch = useDispatch();\n    const [posts, setPosts] = useState('');\n    const [forceOnDelete, setForceOnDelete] = useState(0);\n\n    const deletePost = (id)=>{\n        fetch(API_URL+`/postId=${id}`, {\n            method:\"DELETE\",\n        }).then(()=>{\n            setForceOnDelete(forceOnDelete+1);\n        })\n    }\n\n    const addMealToBasket = (singleMealData) =>{\n        // dispatch data to basket\n        dispatch(ADD_TO_BASKET(singleMealData));\n    }\n\n    useEffect(() => {\n        fetch(API_URL+\"/view\", {\n            method: 'GET',\n        }).then((response)=>{\n            return response.json();\n        }).then((data)=>{\n            setPosts(data);\n        })\n    }, [forceOnDelete])\n\n    return (\n        <div className='view'>\n            <Link className='view__create' to='/create'><AddIcon /></Link>\n            {user && \n            <div className='view__user'>\n                <div className='view__userWrapper'>\n                    <img src={user.imageURL} alt='' />\n                    <h4>Welcome back, {user.displayName}</h4>\n                </div>\n            </div>}\n            <div className='view__wrapper'>\n                <h2>View Recent Publishes</h2>\n                {posts? posts.map((obj, index)=>{\n                    if (obj.privacy === true){\n                    return (<Link to={`/post/${obj._id}`}><div className='view__container'>\n                        <div className='view__add' \n                            onClick={()=>addMealToBasket(obj)}\n                        >\n                            +\n                        </div>\n                        {user && user.email.split('@')[0]===obj.author &&\n                            <div className='userControls'>\n                                <div \n                                    className='view__delete view__control'\n                                    onClick={()=> deletePost(obj._id)}\n                                >Delete</div>\n                                <Link to={`/update/${obj._id}`}><div className='view__update view__control'>Update</div></Link>\n                            </div>}\n                        <img \n                            className='view__image'\n                            src={obj.image? obj.image : 'https://images.unsplash.com/photo-1608501857571-31a43311e342?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1950&q=80'}\n                            alt=''\n                        />\n                        <div className='view__content'>\n                        <div className='view__title' >{obj.title}</div>\n                        <div className='view__author' >Published By: {obj.author}</div>\n                        <div className='view__date' >{new Date(parseInt(obj.published_date)).toDateString()}</div>\n                        <div className='view__directions' >\n                            <ul>\n                                {obj.ingrediants? obj.ingrediants.map((data)=>(\n                                    <li>{data.name}{data.amount? \": \"+data.amount:''}</li>\n                                )) : ''}\n                            </ul>\n                            <span>{obj.instructions? obj.instructions : ''}</span>\n                        </div>\n                    </div>\n                    </div></Link>)\n                } else {\n                    return ('')\n                }}) : (\n                <div className='view__container'>\n                    <img className='view__image' \n                        src='https://images.unsplash.com/photo-1608501857571-31a43311e342?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1950&q=80'\n                        alt=''\n                    />\n                    <div className='view__content'>\n                        <div className='view__title' >Title</div>\n                        <div className='view__author' >Published By: author</div>\n                        <div className='view__date' >date</div>\n                        <div className='view__directions' >\n                            Directions\n                        </div>\n                    </div>\n                </div>\n                )}\n            </div>\n        </div>\n    )\n}\n\nexport default View\n","/home/kam/Documents/projects/groceries/src/TopFive.js",[],"/home/kam/Documents/projects/groceries/src/stateProvider.js",[],["131","132"],"/home/kam/Documents/projects/groceries/src/reducer.js",[],["133","134"],"/home/kam/Documents/projects/groceries/src/firebase.js",[],"/home/kam/Documents/projects/groceries/src/User.js",["135","136","137"],"import React, { useEffect, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport { selectDefault } from './features/reactSlice';\nimport SmallPost from './SmallPost';\nimport './User.css';\nimport SendIcon from '@material-ui/icons/Send';\nimport SettingsIcon from '@material-ui/icons/Settings';\nimport KeyboardArrowLeftIcon from '@material-ui/icons/KeyboardArrowLeft';\n\nconst User = () => {\n\n    const API_URL = 'Http://localhost:9000';\n    const user = useSelector(selectDefault);\n    const [userPosts, setUserPosts] = useState('');\n\n    useEffect(() => {\n        if (user){\n            fetch(API_URL+'/userView/'+`${user.displayName}`, {\n                method:'GET',\n            }).then((response)=>{\n                return response.json()\n            }).then((data)=>{\n                setUserPosts(data);\n            })\n        } // end if\n    }, [user])\n\n    return (\n        <div className='user'>\n            {user && \n            <Link className='user__backArrow' to='/view'>Go Back <KeyboardArrowLeftIcon /></Link>\n            }\n            {user? \n            (\n                <div className='conditional_wrapper'>\n                    <div className='user__wrapper'>\n                        <div className='user__upperContent'>\n                            <img\n                                className='user__image'\n                                src={user.imageURL}\n                                alt=''\n                            />\n                            <div className='user__titleContent'>\n                                <span>{user.displayName}</span>\n                                <span>{user.email}</span>\n                                <span>Date joined</span>\n                                <span>Your Total Posts: {userPosts? userPosts.length : 0}</span>\n                            </div>\n                        </div>\n\n                        <div className='user__subContent'>\n                            <div className='user__imageInput'>\n                                <input placeholder='image url' type='text' /><SendIcon />\n                            </div>\n                            <Link to='/editUser' className='user__settingsLink'>Edit your profile <SettingsIcon /></Link>\n                        </div>\n                    </div>\n                    <div className='user__lowerContent'>\n                        <h3>Your Posts</h3>\n                        <h4>Kept Private</h4>\n                        <div className='user__postsPrivate'>\n                            {userPosts? userPosts.map((post, index)=>{\n                                if (!post.privacy){\n                                    return <SmallPost data={post} reversed={true} userControl={true} />\n                                }\n                            }): <h5>No Private Posts Found</h5>}\n                        </div>\n\n                        <h4>Publicly Shared</h4>\n                        <div className='user__postsPu